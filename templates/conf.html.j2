<!DOCTYPE html>
<html lang="{{ lang }}">
    <head>{{ include.css }}</head>
    <body>
        <div class="container">
            {% include 'home.html.j2' %}
            {% macro language(x) %}
                {{ nth("af/ar/bg/br/ca/co/cs/da/de/en/eo/es/et/fi/fr/he/is/it/lt/lv/nl/no/oc/pl/pt/pt-br/ro/ru/sk/sl/sv/tr/zh/
                                ", "x") }}
            {% endmacro %}
            {% macro compilation_info(x, y) %}
                {{ _("branch %s compiled on %s from commit:::x:y", variant='0') |capitalize }}
            {% endmacro %}
            <h1 class="mt-1 mx-3">
                {{ _("geneweb parameters") |capitalize }}<i class="fa fa-hat-wizard fa-xs text-success ml-2"
   title="{{ _("wizard/wizards/friend/friends/exterior", variant='1') |capitalize }}"
   aria-hidden="true"
   alt="green {{ _("wizard/wizards/friend/friends/exterior", variant='1') |capitalize }} icon"></i>
            </h1>
            <section id="build-info" class="mt-1 mx-3">
                <h2>{{ _("build information") |capitalize }}</h2>
                <div>
                    GeneWeb version{{ _(":") }} {{ version }}
                    <br>
                    {{ compilation_info(branch, compil_date) }} {{ commit_id }}
                </div>
            </section>
            <section id="user-info"  class="mt-3 mx-3">
                <h2>{{ _("user and language variables") |capitalize }}</h2>
                <div>
                    User{{ _(":") }} {{ user.ident }}
                    <br>
                    username{{ _(":") }} {{ user.name }}
                    <br>
                    userkey{{ _(":") }} {{ user.key }}
                    <br>
                    lang: {{ lang.full }} ({{ lang }})
                    <br>
                    lang fallback: {{ lang_fallback }}
                    <br>
                    default_lang: {{ default_lang }}
                    <br>
                    browser_lang: {{ browser_lang }}
                    <br>
                </div>
            </section>
            <section id="gwd-arguments" class="mt-3 mx-3">
                <h2>{{ _("launch arguments of Gwd server") |capitalize }}</h2>
                <div>{{ gwd.arglist }}</div>
                <p>
                    Mode:
                    {% if cgi %}
                        CGI
                    {% else %}
                        Server
                    {% endif %}
                    <br>
                    prefix: /
                    <br>
                    etc_prefix: {{ etc_prefix }}
                    <br>
                    images_prefix: {{ images_prefix }}
                    <br>
                </section>
                <section id="bvar-list" class="mt-3 mx-3">
                    <h2>{{ _("configuration parameters") |capitalize }}</h2>
                    <div>
                        Mode: <b>
                        {% if reorg %}
                            Reorg
                        {% else %}
                            Classic
                        {% endif %}
                    </b>{{ _(":") }}{{ sp }}<tt>
                    {% if reorg %}
                        {{ base.name }}.gwb/etc/{{ base.name }}
                    {% else %}
                        {{ base.name }}
                    {% endif %}
                .gwf</tt>
                <br>
                {{ bvar.list }}
            </div>
        </section>
        {% include 'trl.html.j2' %}
        {% include 'copyr.html.j2' %}
        {% include 'js.html.j2' %}
        {{ query_time }}
    </div>
</body>
</html>
