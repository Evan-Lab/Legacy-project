{{ doctype }}
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <!-- $Id: templm/dag.txt v7.0 2015/04/30 11:14:09 $ -->
        <title>{{ head_title }}</title>
        <meta name="robots" content="none" />
        <meta name="format-detection" content="telephone=no" />
        <meta http-equiv="Content-Type" content="text/html; charset={{ charset }}" />
        <meta http-equiv="Content-Style-Type" content="text/css" />
        <link rel="shortcut icon"
              href="{{ url_for('static/images/', path='favicon_gwd.png') }}" />
        {% include 'css' %}
    </head>
    <body {{ body_prop }} id="dag">
        {% include 'hed' %}
        {{ message_to_wizard }}
        {% if ( ( not cancel_links ) and ( evar.notab != "on" ) ) %}
            {% with %}
                {% set l_o = "image="evar.image"&bd="evar.bd"&color="evar.color"&invert="evar.invert"&new="evar.new %}
                {% with %}
                    {% set l_url1 = "/""m=D&t=L&tl1=hrztl&v="evar.v"&marriage2="evar.marriage2"&sosa="evar.sosa"&xy="evar.xy"&siblings="evar.siblings"&timeline="evar.timeline"&gen="evar.gen"&" %}
                    <div class="menu">
                        {% if ( link_next != "" ) %}
                            <span>
                                <a href="{{ link_next }}&{{ l_o }}"
                                   title="{{ _(next relationship path) }} (&gt;)"
                                   accesskey=">">{{ _(next relationship path) |capitalize }} &gt;&gt;</a>
                                {% if ( evar.i != "" ) %}
                                    {% with %}
                                        {% set l_i = "ei="evar.i"&i="evar.ei %}<a href="/em=R&et=A&spouse=on&{{ l_i }}&{{ l_o }}">{{ _(relationship link/relationship links, variant='1') |capitalize }}</a>
                                        <a href="/em=R&et=M&spouse=on&{{ l_i }}&{{ l_o }}">{{ _(relationship by marriage) |capitalize }}</a>
                                    {% endwith %}
                                {% endif %}
                            </span>
                        {% endif %}
                        {% if ( evar.t == "T" ) %}
                            {% if ( evar.m == "A" ) %}
                                <span>
                                    <a title="{{ _(ascendants tree) |capitalize }} (Y)"
                                       accesskey="Y"
                                       href="{{ prefix_set.iz }}{# foreach not implemented yet #}">{{ _(descendants tree) |capitalize }}</a>
                                </span>
                            {% else %}
                                {% if ( evar.m == "D" ) %}
                                    <span>
                                        <a title="{{ _(tree) }} (Y)"
                                           accesskey="Y"
                                           href="{{ prefix_set.iz }}{# foreach not implemented yet #}">{{ _(ascendants tree) |capitalize }}</a>
                                    </span>
                                    <span>
                                        <a href="{% if ( bvar.access_by_key == "no" ) %}{{ l_url1 }}i={{ evar.i }}{% else %}#{% endif %}"
                                           title="{% if ( bvar.access_by_key != "no" ) %}Link not working atm if access_by_key=yes, sorry!{% endif %}">{{ _(descendant tree view, variant='0') |capitalize }}</a>
                                    </span>
                                {% endif %}
                            {% endif %}
                        {% endif %}
                        {% if ( ( evar.m == "RLM" ) and ( ( evar.i6 == "" ) and ( ( evar.p6 == "" ) and ( evar.t1 != "1" ) ) ) ) %}
                            <span>
                                {% with %}
                                    {% set l_i = "i="evar.i1"&ei="evar.i2 %}<a href="/em=R&et=S&spouse=on&{{ l_i }}&{{ l_o }}">{{ _(shortest path) |capitalize }}</a>
                                    <a href="/em=R&et=A&spouse=on&{{ l_i }}&{{ l_o }}">{{ _(relationship link/relationship links, variant='1') |capitalize }}</a>
                                    <a href="/em=R&et=M&spouse=on&{{ l_i }}&{{ l_o }}">{{ _(relationship by marriage) |capitalize }}</a>
                                </span>
                            {% endwith %}
                        {% endif %}
                    </div>
                {% endwith %}
            {% endwith %}
        {% endif %}
        {% if ( ( evar.notab == "on" ) or ( evar.pos2 != "" ) ) %}
            {% with %}
                {% set dpos = (evar.dpos) if (( evar.dpos != "" )) else ("78") %}
                {% with %}
                    {% set pos1 = (evar.pos1) if (( evar.pos1 != "" )) else ("0") %}
                    {% with %}
                        {% set pos2 = (evar.pos2) if (( evar.pos2 != "" )) else (dpos) %}
                        {% with %}
                            {% set overlap = (evar.overlap) if (( evar.overlap != "" )) else ("10") %}
                            {% if ( ( pos1 != 0 ) or ( pos2 < dag.ncol ) ) %}
                                <div class="menu">
                                    <span>
                                        {% if ( pos1 == 0 ) %}
                                        {% else %}
                                            <a href="{{ prefix_set.iz }}{# foreach not implemented yet #}{% with %}{% set l_pos1 = ( ( pos1 + overlap ) - dpos ) %}pos1={{ l_pos1 }};pos2={{ pos1 + overlap }}">&lt;&lt; {{ l_pos1 }}</a>{% endwith %}{% endif %}{% if ( pos2 >= dag.ncol ) %}&nbsp;{% else %}<a href="{{ prefix_set.iz }}{# foreach not implemented yet #}{% with %}{% set l_pos1 = ( pos2 - overlap ) %}pos1={{ l_pos1 }};pos2={{ ( pos2 - overlap ) + dpos }}">{{ l_pos1 }} &gt;&gt;</a>
                                        {% endwith %}
                                    {% endif %}
                                </span>
                            </div>
                        {% endif %}
                        <pre style="width:{{ dpos }}em;">
{# foreach not implemented yet #}</pre>
                    {% endwith %}
                {% endwith %}
            {% endwith %}
        {% endwith %}
    {% else %}
        {% with %}
            {% set l_arr = ("&uarr;") if (( evar.m == "A" )) else ("&darr;") %}
            {% with %}
                {% set l_a = ("----&gt;8----") if (( evar.em == "R" )) else (_(tree)) %}
                <table border="{{ border }}" width="100%">
                    <tr>
                        <td align="center">
                            <table border="{{ border }}" cellspacing="0" cellpadding="0">
                                {# foreach not implemented yet #}
                            </table>
                        </td>
                    </tr>
                </table>
            {% endwith %}
        {% endwith %}
    {% endif %}
    {% if ( not cancel_links ) %}
        {% include 'tools' %}
        {{ tools_dag() }}
        {% include 'trl' %}
        {% include 'copyr' %}
        {{ query_time }}
    {% endif %}
</body>
</html>
