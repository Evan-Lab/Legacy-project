{{ doctype }}
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <!-- $Id: cousmenu.txt,v 7.00 2014/12/13 16:55:44 mr Exp $ -->
        <title>{{ _(link between) |capitalize }} {{ self }} {{ _(and, variant='0') }}…</title>
        <meta name="robots" content="none" />
        <meta http-equiv="Content-Type" content="text/html; charset={{ charset }}" />
        <meta http-equiv="Content-Style-Type" content="text/css" />
        {% include 'favicon' %}
        {% include 'css' %}
        {% include 'hed' %}
    </head>
    <body {{ body_prop }}>
        {{ message_to_wizard }}
        <h1>
            {{ _(link between) |capitalize }} <a href="/{{ access }}">{{ self }}</a> {{ dates }} {{ _(and, variant='0') }}…
        </h1>
        <h2>{{ evar.t }}</h2>
        {% macro list1(zz) %}
            {% if ( ( pvar.zz != "" ) and ( evar.tzz != evar.t ) ) %}
                <li>
                    <a href="/{{ pvar.zz.access }}">{{ pvar.zz }}</a> {{ pvar.zz.dates }}
                </li>
                {{ list1(( zz + 1) ) }}
            {% endif %}
        {% endmacro %}
        {% macro list() %}
            {% with %}
                {% set li1 = index %}
                {% with %}
                    {% set lt1 = (_(him/her, variant='0')) if (is_male) else (_(him/her, variant='1')) %}
                    {% if ( evar.list == "1" ) %}
                        <ol>
                            {{ list1(1) }}
                        </ol>
                    {% endif %}
                {% endwith %}
            {% endwith %}
        {% endmacro %}
        {{ list() }}
        <div class="menu" style="position:relative">
            <a {{ l1 }}
               href="{{ prefix_base_password }}{% if ( evar.lang != "" ) %}lang={{ evar.lang }}&{% endif %}{# foreach not implemented yet #}">{{ _(tree) }}</a>
        </div>
        <p></p>
        {% include 'trl' %}
        {% include 'copyr' %}
        {{ query_time }}
    </body>
</html>
