{% if ( evar.m != "SND_IMAGE" ) %}
    {% include 'menu_search' %}
{% else %}
    {% if ( not cancel_links ) %}
        {% if ( ( evar.n != "" ) or ( ( evar.i != "" ) or ( ( evar.ip != "" ) or ( evar.m != "" ) ) ) ) %}
            <table style="position:absolute;
                          top:50px;
                          float:{% if ( ( evar.m == "D" ) and ( ( evar.t == "T" ) and ( ( evar.slices == "" ) or ( evar.slices != "on" ) ) ) ) %}left;
                          left:10px;
                          {% else %}right;
                          right:10px;
                          {% endif %}">
                <tr>
                    <td>
                        {% if ( referer != "" ) %}
                            <a href="{{ referer }}">
                                <img src="{{ url_for('static/images/', path='left.png') }}"
                                     left.png
                                     alt="&lt;&lt;"
                                     title="{{ _(back) |capitalize }}">
                            </a>
                        {% endif %}
                        <a href="/">
                            <img src="{{ url_for('static/images/', path='up.png') }}"
                                 up.pnd
                                 alt="^^"
                                 title="{{ _(home) |capitalize }}">
                        </a>
                        <a href="{{ url_set.cgl.on }}" target="_blank"><img style="border-width: thin"
     {{ sp }}
     src="{{ url_for('static/images/', path='right.png') }}"
     right.png
     alt=""
     title="{{ _(cancel GeneWeb links) |capitalize }}"></a>
                    </td>
                </tr>
            </table>
        {% endif %}
        {% if ( ( evar.digest == "" ) and ( evar.m != "SND_IMAGE" ) ) %}
            {{ nl }}
            <div id="trl" style="clear:both;">
                <ul>
                    <li>
                        <a href="{{ url_set.templ }}">default</a>
                    </li>
                    <li>
                        <a href="{{ url_set.templ.templm }}">templm</a>
                    </li>
                </ul>
            </div>
        {% endif %}
    {% endif %}
{% endif %}
<div id="footer" style="clear:both;">
    <table style="border:0;">
        <tr>
            <td>
                v.&nbsp;{{ version }} - &copy;&nbsp;INRIA&nbsp;1998-2020
                <br>
                <a href="{{ url_set.templ }}"
                   title="Switch to {% if ( evar.templ != "templm" ) %}template m{% else %}default template{% endif %}">default</a>
                {{ connections }}
            </td>
            <td style="width:205px">
                <a href="https://github.com/geneweb/geneweb"
                   target="_blank"
                   title="GeneWeb sources on GitHub">
                    <img src="{{ url_for('static/images/', path='logo_bas.png') }}"
                         alt="Logo GeneWeb bas">
                </a>
                {% with %}
                    {% set suffx = ("/"lang) if (( lang != "en" )) %}<a href="https://geneweb.tuxfamily.org/wiki/GeneWeb{{ suffx }}"
   target="_blank"
   title="GeneWeb documentation MediaWiki">
                    <img src="{{ url_for('static/images/', path='logo_bas_mw.png') }}"
                         alt="Manual of GeneWeb on Mediawiki">
                </a>
            </td>
        </tr>
    </table>
</div>
