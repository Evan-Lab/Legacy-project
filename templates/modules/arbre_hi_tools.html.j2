<!-- $Id: modules/arbre_hi_tools.txt v7.0 26/09/2023 04:30:23 $ -->
{% with %}
    {% set evarv = (e.v) if (( e.v != "" )) else ("7") %}
    {% with %}
        {% set evarscl = (e.scale) if (( e.scale != "" )) else ("100") %}
        {% with %}
            {% set evarfs = (e.fs) if (( e.fs != "" )) else ("17") %}
            {% with %}
                {% set maxlev = ("13") if (( e.cpl == "on" )) else ("12") %}
                <div class="d-flex ml-2 mb-2">
                    {% if ( not cancel_links ) %}
                        <div class="btn-toolbar"
                             role="toolbar"
                             aria-label="Toolbar with button groups">
                            <div class="btn-group mx-2">
                                {% if ( evarv >= 4 ) %}
                                    <a href="{{ url_set("v", ( evarv - 1) ) }}"
                                       class="btn btn-outline-primary"
                                       title="(−1) {{ _(visualize/show/hide/summary, variant='1') }} {{ evarv - 1 }} {{ _(generation/generations, variant='1') }}">−</a>
                                {% endif %}
                                <span class="btn btn-outline-primary disabled border-top border-bottom"
                                      title="{{ _(generation/generations, variant='1') }}">{{ evarv }} gen.</span>
                                {% if ( evarv < ( maxlev - 1 ) ) %}
                                    <a href="{{ url_set("v", ( evarv + 1) ) }}"
                                       class="btn btn-outline-primary"
                                       title="(+1) {{ _(visualize/show/hide/summary, variant='1') }} {{ evarv + 1 }} {{ _(generation/generations, variant='1') }} (max {{ maxlev }})">+</a>
                                {% endif %}
                            </div>
                            <div class="mx-2">
                                <a href="{{ url_set("hi", ("I") if (( ( e.hi == "H" ) or ( e.hi == "" ) ))) }}"
                                   class="btn btn-outline-primary">
                                    {% if ( ( e.hi == "H" ) or ( e.hi == "" ) ) %}
                                        I
                                    {% else %}
                                        H
                                    {% endif %}
                                -tree</a>
                                <a href="{% if ( e.sosa != "on" ) %}{{ url_set.sosa.on }}{% else %}{{ url_set.sosa }}{% endif %}"
                                   class="btn btn-outline-primary ml-1"
                                   title="{{ _(visualize/show/hide/summary, variant='1') |capitalize }} {% if ( e.sosa == "on" ) %}ASCII{% else %}HTML{% endif %}">
                                    {% if ( e.sosa != "on" ) %}
                                        HTML
                                    {% else %}
                                        ASCII
                                    {% endif %}
                                box drawing</a>
                                <a href="{% if ( e.sms != "on" ) %}{{ url_set.sms.on }}{% else %}{{ url_set.sms }}{% endif %}"
                                   class="btn btn-outline-primary ml-1"
                                   title="{% if ( e.sms != "on" ) %}{{ _(visualize/show/hide/summary, variant='1') |capitalize }}{% else %}{{ _(visualize/show/hide/summary, variant='2') |capitalize }}{% endif %} {{ _(missing %s:::Sosa, variant='0') }}">·</a>
                                <a href="{% if ( e.cpl != "on" ) %}{{ url_set.cpl.on }}{% else %}{{ url_set.cpl }}{% endif %}"
                                   class="btn btn-outline-primary ml-1"
                                   title="{{ _(visualize/show/hide/summary, variant='1') |capitalize }}{% if ( e.cpl != "on" ) %} {{ _(couples) }}{% else %}{{ _(individuals) }}{% endif %}">
                                    {% if ( e.cpl != "on" ) %}<i class="fa fa-user-plus" aria-hidden="true"></i>{% endif %}
                                    <i class="fa fa-user" aria-hidden="true"></i></a>
                                <a href="{% if ( e.implx != "on" ) %}{{ url_set.implx.on }}{% else %}{{ url_set.implx }}{% endif %}"
                                   class="btn btn-outline-primary ml-1"
                                   title="{% if ( e.implx != "on" ) %}{{ _(implex links) |capitalize }}{% else %}{{ _(do not) |capitalize }} {{ _(implex links) }}{% endif %}">
                                    <i class="fa fa-shuffle fa-fw"></i></a>
                            </div>
                            {% if ( e.sosa == "on" ) %}
                                <div class="btn-group mx-2">
                                    {% if ( evarscl > 0 ) %}
                                        <a href="{{ url_set("scale/fs", ( evarscl - 20) "/"( evarfs - 1 )) }}"
                                           class="btn btn-outline-primary"
                                           title="{{ _(reduce/increase, variant='0') |capitalize }} {{ _(size) }} (-20 %)">−</a>
                                    {% endif %}
                                    <span class="btn btn-outline-primary disabled border-top border-bottom">{{ evarscl }} %</span>
                                    {% if ( evarscl < 200 ) %}
                                        <a href="{{ url_set("scale/fs", ( evarscl + 20) "/"( evarfs + 1 )) }}"
                                           class="btn btn-outline-primary"
                                           title="{{ _(reduce/increase, variant='1') |capitalize }} {{ _(size) }} (+20 %)">+</a>
                                    {% endif %}
                                </div>
                                <div class="btn-group mx-2">
                                    {% if ( evarfs > 5 ) %}
                                        <a href="{{ url_set("fs", ( evarfs - 1) ) }}"
                                           class="btn btn-outline-primary"
                                           title="{{ _(reduce/increase, variant='0') }} {{ _(font size) }}">−</a>
                                    {% endif %}
                                    <span class="btn btn-outline-primary disabled border-top border-bottom">Font {{ evarfs }} px</span>
                                    {% if ( evarfs < 20 ) %}
                                        <a href="{{ url_set("fs", ( evarfs + 1) ) }}"
                                           class="btn btn-outline-primary"
                                           title="{{ _(reduce/increase, variant='1') }} {{ _(font size) }}">+</a>
                                    {% endif %}
                                </div>
                            {% endif %}
                        </div>
                    {% endif %}
                </div>
            {% endwith %}
        {% endwith %}
    {% endwith %}
{% endwith %}
