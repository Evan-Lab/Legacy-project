<!DOCTYPE html>
<html lang="{{ lang }}">
    <head>
        <!-- $Id: calendar.txt v7.1 10/01/2023 20:01:49 $ -->
        <!-- Copyright (c) 1998-2007 INRIA -->
        <title>{{ _("calendar/calendars", variant='1') |capitalize }}</title>
        <meta name="robots" content="none">
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="shortcut icon"
              href="{{ url_for('static', path='images/favicon_gwd.png') }}">
        {% include 'css.html.j2' %}
    </head>
    <body {{ body_prop }}>
        {% include 'hed.html.j2' %}
        {% include 'home.html.j2' %}
        {{ message_to_wizard }}
        <div class="container">
            {% macro fun_day(ccc, nnn) %}
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="dccc1">&lt;</button>
                </td>
                <td>
                    <input type="number"
                           class="form-control text-center"
                           name="dccc"
                           min="1"
                           max="31"
                           value="{{ date.nnn.day }}" />
                </td>
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="dccc2">&gt;</button>
                </td>
            {% endmacro %}
            {% macro fun_month(ccc, nnn, vvv, lll) %}
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="mccc1">&lt;</button>
                </td>
                <td>
                    <select class="form-control h-100 w-100"
                            style="text-align-last:center"
                            name="mccc">{# foreach not implemented yet #}</select>
                </td>
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="mccc2">&gt;</button>
                </td>
            {% endmacro %}
            {% macro fun_year(ccc, nnn) %}
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="yccc1">&lt;</button>
                </td>
                <td>
                    <input class="form-control text-center"
                           name="yccc"
                           size="6"
                           maxlength="6"
                           value="{{ date.nnn.year }}" />
                </td>
                <td>
                    <button class="btn btn-outline-secondary" type="submit" name="yccc2">&gt;</button>
                </td>
            {% endmacro %}
            {% macro fun_julian_year(ccc, nnn) %}
                {% with %}
                    {% set year = (( date.nnn.year - 1 )"/"( date.nnn.year % 10 )) if (( ( date.julian.month < 3 ) or ( ( date.julian.month == 3 ) and ( date.julian.day < 25 ) ) )) else (date.nnn.year) %}
                    <td>
                        <button class="btn btn-outline-secondary" type="submit" name="yccc1">&lt;</button>
                    </td>
                    <td>
                        <input class="form-control text-center"
                               name="yccc"
                               size="6"
                               maxlength="6"
                               value="{{ year }}" />
                    </td>
                    <td>
                        <button class="btn btn-outline-secondary" type="submit" name="yccc2">&gt;</button>
                    </td>
                {% endwith %}
            {% endmacro %}
            {% macro calendar(ccc, nnn, vvv, lll, fun_yyy) %}
                {% if ( ( _("!dates order") == "ddmmyy" ) or ( ( _("!dates order", variant='0') == "ddmmyyyy" ) or ( _("!dates order", variant='0') == "dmyyyy" ) ) ) %}
                    {{ fun_day('ccc', 'nnn') }}{{ fun_month('ccc', 'nnn', 'vvv', 'lll') }}{{ fun_yyy('ccc', 'nnn') }}
                {% else %}
                    {% if ( _("!dates order") == "mmddyyyy" ) %}
                        {{ fun_month('ccc', 'nnn', 'vvv', 'lll') }}{{ fun_day('ccc', 'nnn') }}{{ fun_yyy('ccc', 'nnn') }}
                    {% else %}
                        {{ fun_yyy('ccc', 'nnn') }}{{ fun_month('ccc', 'nnn', 'vvv', 'lll') }}{{ fun_day('ccc', 'nnn') }}
                    {% endif %}
                {% endif %}
                <td>
                    <button class="btn btn-outline-success" type="submit" name="tccc">=</button>
                </td>
            {% endmacro %}
            <div class="row mt-4">
                <div class="mx-auto text-center">
                    <h1>{{ _("calendar/calendars", variant='1') |capitalize }}</h1>
                    <h3 class="mt-4">
                        {{ capitalize(nth(_("(week day)") , date.week_day)) }}
                        {% if ( date.julian_day == today.julian_day ) %}- {{ time }}{% endif %}
                    </h3>
                </div>
            </div>
            <div class="row mt-3">
                <div class="mx-auto text-center">
                    <form method="get" action="{{ action }}">
                        <div class="form-inline">
                            {{ hidden }}
                            <input type="hidden" name="m" value="CAL" />
                            <table class="table-sm table-bordered mx-auto">
                                <thead>
                                    <tr>
                                        <th class="py-2">&nbsp;</th>
                                        {% if ( ( _("!dates order") == "ddmmyy" ) or ( ( _("!dates order", variant='0') == "ddmmyyyy" ) or ( _("!dates order", variant='0') == "dmyyyy" ) ) ) %}
                                            <th class="text-center" colspan="3">{{ _("year/month/day", variant='2') |capitalize }}</th>
                                            <th class="text-center" colspan="3">{{ _("year/month/day", variant='1') |capitalize }}</th>
                                            <th class="text-center" colspan="3">{{ _("year/month/day", variant='0') |capitalize }}</th>
                                        {% else %}
                                            {% if ( _("!dates order") == "mmddyyyy" ) %}
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='1') |capitalize }}</th>
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='2') |capitalize }}</th>
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='0') |capitalize }}</th>
                                            {% else %}
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='0') |capitalize }}</th>
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='1') |capitalize }}</th>
                                                <th class="text-center" colspan="3">{{ _("year/month/day", variant='2') |capitalize }}</th>
                                            {% endif %}
                                        {% endif %}
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <th class="px-2" class="mx-1" scope="row">{{ _("gregorian/julian/french/hebrew", variant='0') |capitalize }}</th>
                                        {{ calendar("g", "gregorian", 12, _("(month)") , "fun_year") }}
                                    </tr>
                                    <tr>
                                        <th class="px-2" scope="row">{{ _("gregorian/julian/french/hebrew", variant='1') |capitalize }}</th>
                                        {{ calendar("j", "julian", 12, _("(month)") , "fun_julian_year") }}
                                    </tr>
                                    <tr>
                                        <th class="px-2" scope="row">{{ _("gregorian/julian/french/hebrew", variant='2') |capitalize }}</th>
                                        {{ calendar("f", "french", 13, _("(french revolution month)") , "fun_year") }}
                                    </tr>
                                    <tr>
                                        <th class="px-2" scope="row">{{ _("gregorian/julian/french/hebrew", variant='3') |capitalize }}</th>
                                        {{ calendar("h", "hebrew", 13, _("(hebrew month)") , "fun_year") }}
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </form>
                    {% if ( date.moon_phase.index != 0 ) %}
                        {{ capitalize(nth(_("moon age/new moon/first quarter/full moon/last quarter") , date.moon_phase.index)" -"sp"<samp>"date.moon_phase.hour_(":")date.moon_phase.minute"</samp> UT
                        ") }}
                    {% endif %}
                    <br>
                    <span>{{ _("moon age/new moon/first quarter/full moon/last quarter", variant='0') |capitalize }}{{ _(":") }}{{ sp }}{{ date.moon_age }}</span>
                    <br>
                    <br>
                    {{ _("julian day") |capitalize }}{{ _(":") }}{{ sp }}
                    {% if ( date.julian_day < 1000 ) %}
                        {{ date.julian_day }}
                    {% else %}
                        {{ date.julian_day.sep1000 }}
                    {% endif %}
                </div>
            </div>
            {% include 'trl.html.j2' %}
            {% include 'copyr.html.j2' %}
        </div>
        {% include 'js.html.j2' %}
        {{ query_time }}
    </body>
</html>
