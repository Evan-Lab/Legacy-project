<!DOCTYPE html>
<html lang="{{ lang }}">
    <head>
        <!-- $Id: perso.txt {{ version }} 08/12/2023 23:04:39 $ -->
        <!-- Copyright (c) 1998-2007 INRIA -->
        <title>
            {% if has_public_name %}
                {% if has_qualifiers %}
                    {{ public_name }} {{ qualifier }}
                {% else %}
                    {{ public_name }} {{ surname }}
                    {% if occ != "" and occ != 0 %}({{ occ }}){% endif %}
                {% endif %}
            {% else %}
                {% if has_qualifiers %}
                    {{ first_name }} {{ qualifier }}
                {% else %}
                    {{ first_name }} {{ surname }}
                    {% if occ != "" and occ != 0 %}({{ occ }}){% endif %}
                {% endif %}
            {% endif %}
        </title>
        {% if bvar.robot_index == "yes" %}
            <meta name="robots" content="index,follow">
        {% else %}
            <meta name="robots" content="none">
        {% endif %}
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon"
              href="{{ url_for("static", path="images/favicon_gwd.png") }}">
        <link rel="apple-touch-icon"
              href="{{ url_for("static", path="images/favicon_gwd.png") }}">
        {% include 'css.html.j2' %}
    </head>
    <body {{ body_prop }}>
        {% include 'hed.html.j2' %}
        {{ message_to_wizard }}<a href="#content" class="sr-only sr-only-focusable">{{ _("skip to main content") |capitalize }}</a>
        <div class="container{% if evar.wide == "on" %}-fluid{% endif %}">
            {% include 'perso_utils.html.j2' %}
            {% include 'menubar.html.j2' %}
            <div id="content" tabindex="-1">
                {% if evar.p_mod == "zz" or bvar.p_mod == "zz" and evar.p_mod == "" or bvar.p_mod == "" and evar.p_mod == "" %}
                    <div class="row">
                        {% with %}
                            {% set op_m = "3" %}
                            {% include 'modules/individu.html.j2' %}
                            {% with %}
                                {% set op_m = "2" %}
                                {% include 'modules/unions.html.j2' %}
                                {% with %}
                                    {% set op_m = "1" %}
                                    {% include 'modules/chronologie.html.j2' %}
                                    {% with %}
                                        {% set op_m = "1" %}
                                        {% include 'modules/notes.html.j2' %}
                                        {% with %}
                                            {% set op_m = "2" %}
                                            {% include 'modules/sources.html.j2' %}
                                            {% if p_mod == "" or p_mod == "zz" %}</div>{% endif %}
                                        <div class="col-lg-4">
                                            {% with %}
                                                {% set op_m = "2" %}
                                                {% include 'modules/arbre_3gen_photo.html.j2' %}
                                                {% with %}
                                                    {% set op_m = "1" %}
                                                    {% include 'modules/fratrie.html.j2' %}
                                                    {% with %}
                                                        {% set op_m = "1" %}
                                                        {% include 'modules/relations.html.j2' %}
                                                    </div>
                                                </div>
                                            {% endwith %}
                                        {% endwith %}
                                    {% endwith %}
                                {% endwith %}
                            {% endwith %}
                        {% endwith %}
                    {% endwith %}
                {% endwith %}
            {% else %}
                <div class="mt-2">
                    {% for i in range(0, p_mod_nbr) %}
                        {% with %}
                            {% set mmi = mm(i) %}
                            {% with %}
                                {% set ooi = oo(i) %}
                                {% if mmi != "" and ooi != 0 %}{{ get_mod(mmi, ooi) }}{% endif %}
                            {% endwith %}
                        {% endwith %}
                    {% endfor %}
                </div>
            {% endif %}
            {% if evar.opt == "misc" %}
                {{ misc_names }}
                <p>
                {% endif %}
                {% if not cancel_links %}
                    {% include 'trl.html.j2' %}
                    {% include 'copyr.html.j2' %}
                    {{ query_time }}
                {% endif %}
            </div>
        </div>
        {% include 'js.html.j2' %}
        <script>
            $(function() {
                $('{{ _('
                    data - toggle = "tooltip"
                    ') }}').tooltip()
            })
        </script>
    </body>
</html>
