                </a>
            {% endif %}
        {% else %}
            {% if e.em == "R" or e.i != "" and e.ei != "" or person_index[1] != "" and person_index[2] != "" %}
                <a class="disabled {% if e.et == "S" %}border rounded{% endif %}"
                   href="/&em=R&et=S{% if e.i != "" and e.ei != "" %}&i={{ e.i }}&ei={{ e.ei }}{% else %}{% if e.ei != "" and e.select != "" %}&i={{ e.select }}&ei={{ e.ei }}{% else %}&i={{ person_index[1] }}&ei={{ person_index[2] }}{% endif %}{% endif %}{% if e_im == 0 %}&im=0{% endif %}{% if e_sp == 0 %}&sp=0{% endif %}{% if e.bd != "" %}&bd={{ e.bd }}{% endif %}{% if e.color != "" %}&color={{ e.color }}{% endif %}"
                   title="{{ _("shortest path") |capitalize }}"
                   accesskey=">"><i class="fas fa-bezier-curve fa-fw"></i><i class="fas fa-arrow-right fa-fw"></i></a>
            {% endif %}
        {% endif %}
        {% if is_printed_by_template and e.i != "" and e.ei != "" or person_index[1] != "" and person_index[2] != "" %}
            <a class="ml-2{% if e.et == "A" %} border rounded{% endif %}"
